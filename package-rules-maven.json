{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "commons-httpclient:commons-httpclient",
        "jaxen:jaxen",
        "log4j:log4j",
        "opensymphony:oscore",
        "org.apache.velocity:velocity",
        "^org.codehaus.jackson"
      ],
      "allowedVersions": "/.+[0-9]+-atlassian-[0-9]+$/"
    },
    {
      "matchPackagePatterns": [
        "^com.atlassian",
        "net.java.dev.activeobjects"
      ],
      "allowedVersions": "/^[0-9]{1,3}\\.[0-9]+\\.[0-9]+(\\.[0-9]+)?$/"
    },
    {
      "matchPackagePatterns": [
        "com.google.code.findbugs:annotations",
        "biz.aQute.bnd:biz.aQute.bndlib"
      ],
      "allowedVersions": "/^[0-9]+\\.[0-9]+\\.[0-9]+(\\.[0-9]+)?$/"
    },
    {
      "matchPackageNames": [
        "org.springframework:org.springframework.beans",
        "org.springframework:org.springframework.context"
      ],
      "allowedVersions": "!/2.5.6/"
    },
    {
      "matchPackageNames": ["com.microsoft.sqlserver:mssql-jdbc"],
      "allowedVersions": "/jre8$/"
    },
    {
      "matchPackageNames": ["commons-io:commons-io"],
      "allowedVersions": "!/1.4-backport-IO-168/"
    },
    {
      "matchPackageNames": ["javax.servlet:servlet-api"],
      "allowedVersions": "!/2.4-20040521/"
    },
    {
      "matchPackageNames": ["c3p0:c3p0"],
      "allowedVersions": "!/0.9.1.2-pbruski/"
    },
    {
      "matchPackageNames": ["com.sun.jersey:jersey-client"],
      "allowedVersions": "!/.+-ea/"
    },
    {
      "matchPackageNames": ["javax.xml.bind:jaxb-api"],
      "allowedVersions": "/^[0-9]+\\.[0-9]+(\\.[0-9]+)?$/"
    },
    {
      "matchPackageNames": ["org.reflections:reflections"],
      "allowedVersions": "!/^0.9.12$/"
    },
    {
      "matchPackageNames": ["com.google.guava:guava"],
      "versioning": "regex:^(?<major>\\d+)(\\.(?<minor>\\d+))?(\\.(?<patch>\\d+))?(-(?<compatibility>.*))?$"
    }
  ]
}
