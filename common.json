{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePresets": [":prHourlyLimit2"],
  "fetchReleaseNotes": false,
  "separateMinorPatch": true,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "groupName": "patches"
    }
  ]
}
